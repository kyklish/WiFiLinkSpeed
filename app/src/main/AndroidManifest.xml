<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="io.github.kyklish.wifilinkspeed">

	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
	<uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
	<uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

	<application
		android:name="io.github.kyklish.wifilinkspeed.App"
		android:allowBackup="true"
		android:icon="@drawable/ic_wifi"
		android:label="@string/app_name"
		android:roundIcon="@drawable/ic_wifi"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name="io.github.kyklish.wifilinkspeed.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<service android:name="io.github.kyklish.wifilinkspeed.ForegroundService" />
	</application>


</manifest>